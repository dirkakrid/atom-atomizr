{
  "name": "atomizr",
  "description": "Converts Sublime Text completions into Atom snippets, and vice versa",
  "version": "0.17.0",
  "license": "MIT",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "sublime text",
    "atom",
    "conversion",
    "atom2subl",
    "subl2atom"
  ],
  "activationCommands": {
    "atom-workspace": [
      "atomizr:convert-sublime-text-snippet-to-atom",
      "atomizr:convert-sublime-text-completions-to-atom",
      "atomizr:toggle-atom-format",
      "atomizr:convert-sublime-text-to-atom",
      "atomizr:convert-atom-to-sublime-text",
      "atomizr:toggle-sublime-sublformat",
      "atomizr:automatic-conversion"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idleberg/atom-atomizr.git"
  },
  "main": "./lib/atomizr",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "cson": "^3.0.2",
    "cson-parser": "^1.3.3",
    "parse-json": "^2.2.0",
    "xml-js": "^0.9.5"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.0.0",
    "jshint": "^2.9.3"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
